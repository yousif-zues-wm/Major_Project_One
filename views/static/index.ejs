.wrapper {
  margin:0 auto;
  text-align:center;
  padding:5%;
  position:relative;
}


.big-circle {
  background-color: rgba(0, 0, 0, 0.2) ;
  border-radius:50%;
  width:80px;
  height:80px;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  transition: 0.3s;
}

.big-circle:hover {
  width:180px;
  height:180px;
}

.big-circle:hover  .small-circle {
    transform: rotate3d(0, 0, 1, 45deg);
}

.fa {
  color: #E3F2FD;
  position: absolute;
  transform: translate(-50%, -50%);
}

.fa:hover {
  color:#90CAF9;
}

.fa-dribbble, .fa-envelope {
   margin-top:30px;
}

.fa-angellist{
   top:50%;
   left:50%;
}
.fa-camera-retro {
   top:50%;
}

.small-circle {
  width:60px;
  height:60px;
  background:#fff;
  border-radius:50%;
  display:inline-block;
  position: absolute;
  top: 50%;
  left: 50%;
  margin:-30px -30px 0;
  transition: 0.3s;
}

.lines:before, .lines:after {
    display: inline-block;
    width: 30px;
    margin-top:-2.5px;
    margin-left:-15.5px;
    height: 3px;
    background: #F74A84;
    /*border-radius: 0.28571rem;*/
    transition: 0.3s;
    position: absolute;
    left: 50%;
    top:50%;
    content: '';
    -webkit-transform-origin: 7.5px
}

.lines:after {
    margin-left:-7px;
    margin-top:-10px;
    transform: rotate3d(0, 0, 1, 90deg);
    -webkit-backface-visibility: hidden;
    /*outline: 1px solid transparent;*/
}
